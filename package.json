{
  "name": "kss-sui-template",
  "version": "0.1.0",
  "description": "KSS template for SUI Framework",
  "main": "./template/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "npm run dist:sass && npm run dist:optimize",
    "dist:sass": "node-sass --output-style nested src/scss/index.scss template/public/sui.css",
    "dist:optimize": "cssnano template/public/sui.css template/public/sui.min.css --no-reduceIdents --no-mergeIdents --no-zindex --no-discardUnused"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUI-Framework/kss.sui.template.git"
  },
  "keywords": [
    "kss",
    "template",
    "sui",
    "shibsted",
    "suiengineers"
  ],
  "devDependencies": {
    "concurrently": "^2.0.0",
    "cssnano-cli": "^1.0.3",
    "gh-pages": "^0.11.0",
    "kss": "^2.4.0",
    "live-server": "^0.9.2",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1"
  },
  "author": "SUI Engineers | @suiengineers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SUI-Framework/kss.sui.template/issues"
  },
  "homepage": "https://github.com/SUI-Framework/kss.sui.template#readme"
}
